{
  "name": "khs1994/flysystem-onedrive",
  "description": "A flysystem driver for OneDrive",
  "keywords": [
    "sdk",
    "api"
  ],
  "homepage": "https://github.com/khs1994-php/flysystem-onedrive",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "khs1994",
      "email": "khs1994@khs1994.com",
      "homepage": "https://khs1994.com"
    }
  ],
  "support": {
    "issues": "https://github.com/khs1994-php/flysystem-onedrive/issues"
  },
  "require": {
    "php": "^7.1.3",
    "nicolasbeauvais/flysystem-onedrive": "~1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "suggest": {
    "ext-xdebug": "*"
  },
  "autoload": {
    "psr-4": {
      "League\\Flysystem\\OneDrive\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "League\\Flysystem\\OneDrive\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "18.06-dev"
    },
    "laravel": {
      "providers": [
        "League\\Flysystem\\OneDrive\\ServiceProvider"
      ]
    }
  }
}
